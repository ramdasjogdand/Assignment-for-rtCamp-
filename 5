
response=$(curl -Is https://example.com | head -n 1)
if [[ "$response" == *"200 OK"* ]]; then
    echo "example.com is up and healthy!"
    echo "Opening example.com in a browser..."
    xdg-open https://example.com  # Replace with the appropriate command for your system/browser
else
    echo "example.com is currently unavailable."
fi
